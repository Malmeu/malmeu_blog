{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/404/?$",
			"dest": "_render"
		},
		{
			"src": "^/about/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/articles/new/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/articles/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/articles/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/comments/?$",
			"dest": "_render"
		},
		{
			"src": "^/admin/login/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/articles/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/newsletter/?$",
			"dest": "_render"
		},
		{
			"src": "^/blog/?$",
			"dest": "_render"
		},
		{
			"src": "^/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/rss\\.xml/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		},
		{
			"src": "^/.*$",
			"dest": "_render",
			"status": 404
		}
	]
}